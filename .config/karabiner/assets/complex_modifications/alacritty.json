{
  "title": "Alacritty",
  "rules": [
    {
      "description": "Alacritty: Left Command to Left Control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io.alacritty"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Alacritty: Left Control+Tab to Left Command+Tab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
                "mandatory": ["left_control"]
            }
          },
          "to": [
              {
                "key_code": "tab",
                "modifiers": ["left_command"]
              }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io.alacritty"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Alacritty: Left Control+Spacebar to Open Alfred",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
                "mandatory": ["left_control"]
            }
          },
          "to": [
              {
                "shell_command": "open -a 'Alfred 4.app'"
              }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io.alacritty"
              ]
            }
          ]
        }
      ]
    },
